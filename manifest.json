{
    "manifest_version": 2,
    "name": "Personal Dashboard",
    "version": "1.0.0",
    "description": " A personal dashboard for your new tab page",
    "browser_action": {
        "default_icon": "./icon.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src-elem 'self' https://apis.google.com; object-src 'self'"
}